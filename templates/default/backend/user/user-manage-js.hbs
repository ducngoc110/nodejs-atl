{{# each users }}
    <tr>
        <td class="uk-text-center">
            <input type="checkbox" class="atl-checkbox-child-js" />
        </td>
        <td class="uk-text-center">
            <img class="md-user-image" style="height: 34px;" src="{{#if meta.avatar}}/uploads/user/{{meta.avatar}}{{else}}/assets/backend/img/blank.png{{/if}}">
        </td>
        <td>
            {{ email }}
        </td>
        <td>
            {{#each ../settings.listOffices}}
            {{#compare @key ../meta.office operator="===" }}{{../this}}{{/compare}}
            {{/each }}
        </td>
        <td class="uk-text-center">
            {{#compare status 'on' operator="==="}}
            <span class="uk-badge">Active</span>
            {{/compare}}
            {{#compare status 'off' operator="==="}}
            <span class="uk-badge uk-badge-danger">Deactive</span>
            {{/compare}}
        </td>
        <td class="uk-text-center">
            {{#each ../settings.listRoles}}
            {{#compare @key ../roles operator="===" }}<span class="uk-badge {{#compare @key 'admin' operator="==="}}uk-badge-danger{{/compare}}">{{../this}}</span>{{/compare}}
            {{/each }}
        </td>
        <td class="uk-text-center">
            <a href="/user-edit/{{ _id }}">
                <i class="md-icon material-icons">edit</i>
            </a>
            <a href="#" class="atl-manage-user-delete" data-id="{{ _id }}">
                <i class="md-icon material-icons">delete</i>
            </a>
        </td>
    </tr>
{{/each }}